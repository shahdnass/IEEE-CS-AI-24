   "source": [
    " import numpy as np\n",
    "array = np.arange(2, 33, 2).reshape((4, 4))\n",
    "\n",
    "mean = np.mean(array)\n",
    "std = np.std(array)\n",
    "\n",
    "lower_bound = mean - 0.5 * std\n",
    "upper_bound = mean + 0.5 * std\n",
    "\n",
    "result = array[(array >= lower_bound) & (array <= upper_bound)]\n",
    "\n",
    "print(result)\n"
   ]
